<script>
window.token = "<%= @token %>"
window.signedIn = <%= @signed_in %>
<%= if @signed_in do %>
window.userID = "<%= @user_id %>"
<% end %>
</script>
<style>
#sign-in-with-google {
  display: block;
  width: 191px;
  height: 46px;
  background-size: cover;
  background-repeat: no-repeat;
  background-image: url("<%= static_path(@conn, "/images/btn_google_signin_dark_normal_web.png") %>");
}
#sign-in-with-google:hover {
  background-image: url("<%= static_path(@conn, "/images/btn_google_signin_dark_focus_web.png") %>");
}
#sign-in-with-google:active {
  background-image: url("<%= static_path(@conn, "/images/btn_google_signin_dark_pressed_web.png") %>");
}
</style>
<div id="root"></div>
<script src="<%= static_path(@conn, "/js/app.js") %>"></script>
